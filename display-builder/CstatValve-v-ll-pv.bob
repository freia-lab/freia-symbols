<?xml version="1.0" encoding="UTF-8"?>
<display version="2.0.0">
  <name>valve-v-l</name>
  <width>45</width>
  <height>24</height>
  <background_color>
    <color red="0" green="0" blue="0" alpha="0">
    </color>
  </background_color>
  <grid_visible>false</grid_visible>
  <widget type="group" version="2.0.0">
    <name>Group_7</name>
    <y>2</y>
    <width>50</width>
    <height>20</height>
    <style>3</style>
    <transparent>true</transparent>
    <actions>
      <action type="open_display">
        <file>../CmdPanels/CstatH-Valve.bob</file>
        <target>standalone</target>
        <description>Open Display</description>
      </action>
    </actions>
    <widget type="symbol" version="2.0.0">
      <name>Symbol_7</name>
      <pv_name>$(P)-$(S):$(V):sOn</pv_name>
      <symbols>
        <symbol>../cryogenics/valve_general_closed_RED.svg</symbol>
        <symbol>../cryogenics/valve_general_GREEN.svg</symbol>
      </symbols>
      <x>28</x>
      <y>3</y>
      <width>20</width>
      <height>15</height>
      <rotation>-90.0</rotation>
    </widget>
    <widget type="label" version="2.0.0">
      <name>Label_10</name>
      <text>$(V)</text>
      <y>3</y>
      <width>32</width>
      <height>15</height>
      <font>
        <font family="Source Sans Pro" style="REGULAR" size="12.0">
        </font>
      </font>
      <foreground_color>
        <color name="BLACK" red="0" green="0" blue="0">
        </color>
      </foreground_color>
      <wrap_words>false</wrap_words>
      <rules>
        <rule name="Background" prop_id="background_color" out_exp="false">
          <exp bool_exp="pv0==1">
            <value>
              <color name="MINOR" red="252" green="242" blue="17">
              </color>
            </value>
          </exp>
          <pv_name>$(P)-$(S):$(V):sOn.SEVR</pv_name>
        </rule>
        <rule name="Transparency" prop_id="transparent" out_exp="false">
          <exp bool_exp="pv0==1">
            <value>false</value>
          </exp>
          <pv_name>$(P)-$(S):$(V):sOn.SEVR</pv_name>
        </rule>
      </rules>
    </widget>
    <widget type="action_button" version="3.0.0">
      <name>Action Button_7</name>
      <actions>
        <action type="open_display">
          <file>../../m-epics-ioc-gersemi-freia/opi/CmdPanels/CstatV-Valve.bob</file>
          <macros>
            <v_name>$(P)-$(S):$(V)</v_name>
          </macros>
          <target>standalone</target>
          <description>Open Display</description>
        </action>
      </actions>
      <text></text>
      <width>45</width>
      <height>20</height>
      <transparent>true</transparent>
      <tooltip>$(P)-$(S):$(V)</tooltip>
    </widget>
  </widget>
</display>
